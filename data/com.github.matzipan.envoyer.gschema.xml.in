<?xml version="1.0" encoding="utf-8"?>
<schemalist>
  <schema path="/com/github/matzipan/envoyer/" id="@app-id@" gettext-domain="@gettext-package@">
    <key name="window-width" type="i">
      <default>600</default>
      <summary>Window width</summary>
    </key>
    <key name="window-height" type="i">
      <default>400</default>
      <summary>Window height</summary>
    </key>
    <key name="is-maximized" type="b">
      <default>false</default>
      <summary>Window maximized state</summary>
    </key>
    <key name="position-x" type="i">
      <default>600</default>
      <summary>x axis position on screen</summary>
      <description>The last recorded x position of the application window.</description>
    </key>
    <key name="position-y" type="i">
      <default>600</default>
      <summary>y axis position on screen</summary>
      <description>The last recorded y position of the application window.</description>
    </key>
  </schema>
</schemalist>